<body class="leftMenu nav-collapse">
    <div id="wrapper">

      {{ include('AppCapitaineBundle:elements:header.html.twig')}}

        <!--
        /////////////////////////////////////////////////////////////////////////
        //////////     SLIDE LEFT CONTENT     //////////
